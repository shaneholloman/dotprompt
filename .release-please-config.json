      "bump-patch-for-minor-pre-major": true,
      "draft": false,
      "prerelease": false,
      "path": "rs",
      "extra-files": [
        {
          "type": "toml",
          "path": "smoke/Cargo.toml",
          "jsonpath": "$.package.version"
        }
      ]
    },
    "dotprompt-java": {
